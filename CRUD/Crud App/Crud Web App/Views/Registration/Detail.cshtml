@model Registration
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<form method="post" asp-action="Detail">
	@* <input type="hidden" asp-for="Id" /> *@
	<div class="border p-5 mt-4 d-flex justify-content-around">
		<div>

			<div action="mb-3">
				<input asp-for="Name" action="form-control" disabled />
			</div>

			<div action="mb-3">
				<input asp-for="Email" action="form-control" disabled />
			</div>

			<div action="mb-3">
				<input asp-for="Password" action="form-control" disabled />

			</div>

			<div action="mb-3">
				<input asp-for="Phone" action="form-control" disabled />
			</div>


			<div class="d-flex justify-content-around">
				<a class="btn btn-dark" asp-controller="Registration" asp-action="Index">Back to Previous</a>
			</div>
		</div>
	</div>
</form>